{
    "name": "laradumps/laradumps",
    "description": "A Dump Component for Laravel.",
    "homepage": "https://github.com/laradumps/laradumps",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Luan Freitas",
            "email": "luanfreitas10@protonmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:laradumps-next/laradumps-core.git"
        }
    ],
    "require": {
        "php": "^8.0",
        "nunomaduro/termwind": "^1.15.1",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/mail": "^9.0|^10.0",
        "laradumps/laradumps-core": "dev-main"
    },
    "require-dev": {
        "orchestra/testbench": "^7.22 | ^8.0",
        "symfony/var-dumper": "^5.4 | ^6.2.5",
        "nunomaduro/larastan": "^2.4",
        "pestphp/pest": "^2.2",
        "laravel/pint": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "LaraDumps\\LaraDumps\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaraDumps\\LaraDumps\\Tests\\" : "tests"
        }
    },
    "scripts": {
        "test:pint": "./vendor/bin/pint --test",
        "pint:fix": "./vendor/bin/pint",
        "test:types": "./vendor/bin/phpstan analyse --ansi --memory-limit=-1",
        "test:tests": "./vendor/bin/pest --parallel --order-by random",
        "verify": [
            "@cs-fixer",
            "@test:types",
            "@test:tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraDumps\\LaraDumps\\LaraDumpsServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
